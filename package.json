  "bugs": {
    "url": "https://github.com/holyshared/apollo-server/issues"
  },
  "homepage": "https://github.com/holyshared/apollo-server#readme",
  "dependencies": {
    "apollo-boost": "^0.4.7",
    "apollo-server-express": "^2.10.0",
    "clean-webpack-plugin": "^3.0.0",
    "express": "^4.17.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "nodemon": "^2.0.2",
    "ts-loader": "^6.2.1",
    "typescript": "^3.7.5",
